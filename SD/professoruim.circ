ARCHITECTURE trans OF bcd_7seg IS
BEGIN
PROCESS (entra)
BEGIN
CASE entra IS
WHEN "0000" =&gt;
sai &lt;= NOT("1111110");
WHEN "0001" =&gt;
sai &lt;= NOT("0110000");
WHEN "0010" =&gt;
sai &lt;= NOT("1101101");
