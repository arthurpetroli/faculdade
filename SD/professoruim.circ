&#13;
LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
ENTITY count4 IS
PORT (
clk : IN STD_LOGIC;
rst : IN STD_LOGIC;
q : OUT STD_LOGIC_VECTOR(3 DOWNTO 0);
carr : OUT STD_LOGIC
);
END ENTITY count4;
ARCHITECTURE trans OF count4 IS
SIGNAL count : STD_LOGIC_VECTOR(3 DOWNTO 0);
BEGIN

PROCESS (clk, rst)
BEGIN
IF (rst = '1') THEN
count &lt;= "0000";
ELSE
IF (clk'EVENT AND clk = '1') THEN
IF (count ="1001") THEN
carr&lt;='1';
ELSE
carr&lt;='0';
END IF;
IF (count &gt;= "1001") THEN
count &lt;= "0000";
ELSE
count &lt;= count + "0001";
END IF;
END IF;
END IF;
END PROCESS;
q &lt;= count;
END ARCHITECTURE trans;</vhdl>
